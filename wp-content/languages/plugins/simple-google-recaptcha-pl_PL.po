#: simple-google-recaptcha.php:441
msgid "Error"
msgstr "Błąd"

#: simple-google-recaptcha.php:478
msgid "is enabled"
msgstr "jest włączony"

#: simple-google-recaptcha.php:484
msgid "is disabled"
msgstr "jest wyłączony"

#: simple-google-recaptcha.php:442
msgid "verification failed"
msgstr "weryfikacja nie powiodła się"

